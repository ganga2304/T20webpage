<h1>Displaying all the Team Details</h1>

<table>
<thead>
<tr>
<th>TeamName</th>
<th>TeamCountry</th>
<th>Founded</th>
<th>Description</th>
<th colspan="3">Actions</th>
</tr>
</thead>
<tbody>
<% @teams.each do |team| %>
    <tr>
    <td><%= team.name %></td>
    <td><%= team.country %></td>
    <td><%= team.founded %></td>
    <td><%= team.description %></td>
    <td><%= link_to 'show',team_path(team) %></td>
    <td><%= link_to 'edit',edit_team_path(team) %></td>
    <td><%= link_to 'delete',team_path(team),method: :delete, data: {confirm: "Are you sure ?"} %></td>
    </tr>
</tbody>
<% end %>
</table>