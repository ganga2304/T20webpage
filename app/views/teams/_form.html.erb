<%if @team.errors.any?%>
  <h2>The following errors prevented the team from being saved</h2>
  <ul>
  <% @team.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
<% end %>
<%= form_with(model: @team) do |f| %>
    <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
    </p>
    <p>
    <%= f.label :country %><br>
    <%= f.text_field :country %>
    </p>

    <p>
    <%= f.label :founded %><br>
    <%= f.number_field:founded %>
    </p>
    <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
    </p>
    <p>
    <%= f.submit "Create team" %>
    </p>
<% end %>