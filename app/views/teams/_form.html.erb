<%= form_with model: @article do |f| %>
  <p>
    <%= f.label :name %><br/>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :country %><br/>
    <%= f.text_field :country %>
  </p>

  <p>
    <%= f.label :founded %><br/>
    <%= f.number_field :founded %>
  </p>
  <p>
    <%= f.label :description %><br/>
    <%= f.text_area :description %>
  </p>
  

  <p>
    <%= f.submit %>
  </p>
<% end %>